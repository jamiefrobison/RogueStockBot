                <h4>Stock Updates for the 100 most recent item updates</h4>
                <div class="table-wrapper">
                    <table class="alt">
                        <thead>
                            <tr>
                                <th>Time</th>
                                <th>Item</th>
                                <th>Stock</th>
                                <th>Link</th>
                            </tr>
                        </thead>
                        <tbody id="data_from_log">
                            <% data.item_info.forEach((entry, index)=>{%>
                            <tr>
                                <td><%=entry.time%></td>
                                <td><%=entry.name%></td>
                                <td>
                                    <% entry.items.forEach((item)=>{%>
                                    <p><%=item%></p>
                                    <%});%>
                                </td>
                                <td>
                                    <a href="<%=entry.link%>" target="_blank">Link</a>
                                </td>
                            </tr>
                            <%});%>
                        </tbody>
                    </table>
