{
    "extends": ["airbnb-base", "prettier"],
     "parserOptions": {
       "ecmaVersion": 2018,
       "sourceType": "module"
     },
     "env": {
       "browser": true,
       "node": true,
       "es6": true
     },
     "plugins": [],
     "rules": {
       "no-unused-vars": [
         "error",
         {
           "vars": "local",
           "args": "none"
         }
       ],
       "no-console": "off",
       "no-plusplus": "off",
       "no-underscore-dangle": "off"
     }
   }